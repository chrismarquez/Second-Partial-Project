
public class Grid {

	protected GameObject[][] grid;
	
	public Grid() {
		this.grid = new GameObject[10][10];
		for (int i = 0; i < 10; i++) {
			for (int j = 0; j < 10; j++) {
				this.grid[i][j] = new Blank(Integer.toString(i)+Integer.toString(j));
			}
		}
	}
	
	public GameObject[][] getGrid() {
		return this.grid;
	}
	
}
