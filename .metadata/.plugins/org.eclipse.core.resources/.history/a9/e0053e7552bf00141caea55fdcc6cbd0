
public class Grid {

	protected GameObject[][] grid;
	
	public Grid() {
		grid = new GameObject[10][10];
		for (int i=0; i<this.grid.length; i++){
			for(int j=0; j<this.grid[i].length; j++){
				//Falta hacer sistema de coordenadas para Blank
				this.grid[i][j] = new Blank(" "); 
			}
		}
	}

	public void setGameObject (String coord, GameObject gameObject){
		for (int i=0; i<this.grid.length; i++){
			for(int j=0; j<this.grid[i].length; j++){
				if (this.grid[i][j] == gameObject(coord).getCoord()){
					
				}
			}
		}
	}
	
}
