
public class Ships extends GameObject{
	
	private boolean orientation;
	private	int 	length;
	
	public Ships(String coord,boolean orientation,int length){
		super(coord);
		this.orientation = orientation;
		this.length = length;
	}

	public boolean getOrientation() {
		return orientation;
	}

	public int getLength() {
		return length;
	}




}
